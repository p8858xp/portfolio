<h3> ADD A REVIEW </h3>
<form method = "POST">
    <label>Course Number:</label><input type = "text" name = "courseNumber">
    <br>
    <label>Course Name:</label><input type = "text" name = "courseName">
    <br>
    <label>Semester:</label><select name="semester">
          <option value="Fall">Fall</option>
          <option value="Spring">Spring</option>
        </select>
    <br>
    <label>Year:</label><input type = "text" name = "year">
    <br>
    <label>Professor:</label><input type = "text" name = "professor">
    <br>
    <label>Review:</label><input type = "text" name = "review">
    <br>
    <br>
    <input type="submit" value="Submit">
</form>
